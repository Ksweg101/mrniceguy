{
  "manifest_version": 2,
  "name": "MrNiceGuy",
  "description": "Replaces bad words with nice words",
  "homepage_url": "https://github.com/mdn/webextensions-examples/tree/master/emoji-substitution",
  "version": "0.8",
  "icons": {
    "48": "icons/LogoChild.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./emojiMap.js", "./substitute.js"]
    }
  ]
}
